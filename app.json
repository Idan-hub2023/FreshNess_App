{
  "expo": {
    "name": "Freshness",
    "slug": "Freshness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/log.png",
    "scheme": "freshness",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.freshness.app",
      "versionCode": 1,
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/log.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.freshness.app",
      "supportsTablet": true,
      "usesAppleSignIn": false
    },
    "plugins": [
      "expo-splash-screen",
      "expo-notifications",
      "expo-font",
      "expo-web-browser",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "${MAPBOX_DOWNLOADS_TOKEN}"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.7.22",
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "97d46ce2-8e8b-4791-bd12-0a6d918aa4cb"
      }
    }
  }
}
