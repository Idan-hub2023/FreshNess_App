{
  "expo": {
    "name": "Freshness",
    "slug": "Freshness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/log.png",
    "scheme": "freshness",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/log.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E90FF"
    },
    "android": {
      "package": "com.freshness.app",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "plugins": [
      "expo-font",
      "expo-web-browser",
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "${MAPBOX_DOWNLOADS_TOKEN}"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "11d679a5-8318-4d0b-918e-db27b0831d31"
      }
    }
  }
}
